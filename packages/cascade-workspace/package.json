{
  "name": "cascade-workspace",
  "version": "0.1.0",
  "description": "Next.js workspace application for Cascade Flow Visualizer",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@cascade/graph": "workspace:*",
    "@cascade/chat": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "yaml": "^2.3.4",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.5.1",
    "commander": "^12.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.9.0",
    "playwright": "^1.52.0",
    "tsx": "^4.7.0",
    "vitest": "^3.2.1"
  }
} 